<div class="section">
	<div class="row">
		<div class="col s12 m6 offset-m3">
			<div class="card">
				<div class="card-content">
					<span class="card-title">Email Plan</span>

					<p>
					  <strong>Name:</strong>
					  <%= @email_plan.name %>
					</p>

					<p>
					  <strong>Expiration:</strong>
					  <%= @email_plan.expiration %>
					</p>

				</div>
				<div class="card-action">
					<%= link_to 'Edit', edit_email_plan_path(@email_plan) %>
					<%= link_to 'Back', email_plans_path %>
				</div>
			</div>
		</div>
	</div>
</div>